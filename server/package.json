{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc && cp package.json dist/",
    "dev": "yarn migrate && yarn build && concurrently \"tsc -w\" \"nodemon dist/server.js\"",
    "start": "yarn migrate && yarn build && cd dist && node server.js",
    "prettier:check": "prettier --check ./",
    "typeorm": "node --require ./node_modules/ts-node/register ./node_modules/typeorm/cli.js --config src/connectors/db.ts",
    "typeorm:runcmd": "ts-node --transpile-only ./node_modules/typeorm/cli.js --config src/connectors/db.ts ",
    "run:task": "ts-node --transpile-only ",
    "migrate": "yarn typeorm:runcmd migration:run",
    "migrate:rollback": "yarn typeorm:runcmd migration:revert",
    "migrate:generate": "yarn typeorm:runcmd migration:generate -- -n ",
    "initTestDb": "docker run --name hfu_test -p 5988:5432 -e POSTGRES_PASSWORD=admin -e POSTGRES_DB=hfu_test -d postgis/postgis:12-3.2",
    "test": "jest --passWithNoTests --verbose",
    "test:watch": "jest --watchAll --verbose"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "chalk": "^5.0.1",
    "class-validator": "^0.12.2",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-validator": "^6.14.0",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "nodemailer": "^6.6.3",
    "nodemailer-sendgrid": "^1.0.3",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.3",
    "redis": "^4.0.4",
    "reflect-metadata": "^0.1.13",
    "response-time": "^2.3.2",
    "swagger-ui-express": "^4.3.0",
    "ts-node": "^10.5.0",
    "typeorm": "^0.2.41",
    "typescript": "^4.1.4",
    "fishery": "^2.2.2",
    "supertest": "^6.2.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "@twgdev/prettier-config": "^1.0.2",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.7",
    "@types/express-validator": "^3.0.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/morgan": "^1.9.1",
    "@types/node": "^17.0.8",
    "@types/nodemailer": "^6.4.4",
    "@types/nodemailer-sendgrid": "^1.0.0",
    "@types/redis": "^4.0.11",
    "@types/response-time": "^2.3.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-twg": "1.0.4",
    "eslint-plugin-html": "^6.1.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^27.5.1",
    "msw": "^0.39.2",
    "prettier": "^2.2.1",
    "ts-jest": "^27.1.4"
  },
  "resolutions": {
    "tslib": "^2.0"
  },
  "peerDependencies": {
    "@types/node": "^14.14.25"
  },
  "prettier": "@twgdev/prettier-config"
}
